{
  "_comment": "cspell:disable",
  "name": "taskflow-backend",
  "version": "2.0.0",
  "description": "TaskFlow AI Backend with Function Calling",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
  "start:dev": "cross-env NODE_ENV=development PORT=3333 nodemon server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "dev": "npm run start:dev",
    "prod": "npm run start:prod",
    "test": "echo \"No tests yet\" && exit 0",
    "health": "curl http://localhost:3333/health || echo 'Server not running'",
    "logs": "tail -f logs/server.log",
    "clean": "rm -rf node_modules package-lock.json"
  },
  "keywords": [
    "ai",
    "tasks",
    "claude",
    "firebase",
    "productivity"
  ],
  "author": "atiaron",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-slow-down": "^3.0.0",
    "firebase-admin": "^13.4.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
